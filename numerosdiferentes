num1=int(input("Digite o primeiro número inteiro:"))
num2=int(input("Digite o segundo número inteiro:"))

if (num1==num2):
    print("Os numeros são IGUAIS, Números lidos:",num1,"e",num2)

if(num1!=num2):
    print("Os números são DIFERENTES, Números lidos:", num1, "e", num2)
    
if(num1!=num2):
     if(num1>num2):
        print("O maior número é:", num1)
        print("O menor número é:", num2)
if(num2>num1):
        print("O maior número é:", num1)
        print("O menor número é:", num2)