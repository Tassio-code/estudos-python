import random

sorteio = random.randint(1,8)

if(sorteio==1):
    print("Sim.")
    
elif(sorteio==2):
    print("N達o.")
    
elif(sorteio==3):
    print("Provavelmente sim.")

elif(sorteio==4):
    print("Provavelmente n達o.")

elif(sorteio==5):
    print("N達o posso prever agora.")
    
elif(sorteio==6):
    print("Concentre-se e pergunte novamente.")
    
elif(sorteio==7):
    print("Melhor n達o te dizer agora.")
    
else:
    print("Pergunte novamente mais tarde.")